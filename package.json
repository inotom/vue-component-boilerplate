{
  "name": "vue-component-boilerplate",
  "version": "1.4.0",
  "description": "",
  "main": "dist/hello-vue.umd.js",
  "cdn": "dist/hello-vue.min.js",
  "config": {
    "dist_dir": "dist",
    "src_dir": "src"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "prebuild": "rimraf $npm_package_config_dist_dir/hello-vue.* && npm run clean && npm test",
    "build": "NODE_ENV=production webpack -p --config config/webpack.prod.js",
    "clean": "rimraf '**/.DS_Store'",
    "lint": "eslint $npm_package_config_src_dir",
    "prettier": "prettier --write 'src/**/*.js'",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inotom/vue-component-boilerplate.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "inotom (http://www.serendip.ws/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inotom/vue-component-boilerplate/issues"
  },
  "homepage": "https://github.com/inotom/vue-component-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "autoprefixer": "9.6.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.2",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.17.1",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "vue": "2.6.10",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.5.1",
    "webpack-merge": "4.2.1"
  }
}
